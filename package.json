{
  "name": "ecommerce-analytics-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "install:pods": "cd ios && pod install && cd ..",
    "test": "jest",
    "lint": "eslint .",
    "compile": "tsc",
    "prepare": "husky install",
    "build:all": "eas build --profile preview --platform all",
